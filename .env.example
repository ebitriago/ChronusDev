# ================================================
# ChronusDev + ChronusCRM - Environment Variables
# ================================================
# Copy this file to .env and fill in your values

# Security
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# ChronusDev API URLs (for external access via domain)
CHRONUSDEV_API_URL=https://api.chronusdev.tudominio.com
CHRONUSCRM_API_URL=https://api.crm.tudominio.com

# ChronusDev Token (for CRM -> Dev integration)
CHRONUSDEV_TOKEN=token-admin-123

# ================================================
# AssistAI Integration
# ================================================
ASSISTAI_API_URL=https://public.assistai.lat
ASSISTAI_API_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...your-token-here
ASSISTAI_TENANT_DOMAIN=your-tenant-domain
ASSISTAI_ORG_CODE=your-org-code
